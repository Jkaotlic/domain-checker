{
  "name": "domain-checker",
  "version": "0.1.0",
  "description": "Next.js service for subdomain discovery via 10 free OSINT sources, DNS brute-force, zone transfers, and reverse DNS lookups.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jkaotlic/domain-checker.git"
  },
  "homepage": "https://github.com/Jkaotlic/domain-checker#readme",
  "bugs": {
    "url": "https://github.com/Jkaotlic/domain-checker/issues"
  },
  "keywords": [
    "subdomain-discovery",
    "osint",
    "dns-bruteforce",
    "reverse-dns",
    "nextjs",
    "typescript"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "ci": "npm run lint && npm run typecheck && npm run test"
  },
  "dependencies": {
    "ioredis": "^5.3.2",
    "lru-cache": "^10.0.0",
    "next": "16.1.6",
    "pino": "^10.3.0",
    "prom-client": "^15.0.0",
    "psl": "^1.15.0",
    "punycode": "^2.3.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jest": "^30.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "typescript": "5.9.3"
  }
}
